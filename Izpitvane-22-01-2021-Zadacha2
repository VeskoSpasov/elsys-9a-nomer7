    #main.consult.py
import calculate 

days = int(input("Naishete dnite: "))
print (calculate(days))

    #calculate.py
def calculate(days):
    if days >= 60:
        return days*300
    elif days < 60 and days >= 30:
        return days*280
    elif days < 30 and days >= 14:
        return days*250
    else:
        return days*200
